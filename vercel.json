{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "client/build",
  "builds": [
    {
      "src": "api/auth.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "github": {
    "silent": true
  }
}